{
  "name": "bombayjs-server",
  "version": "1.0.0",
  "description": "bombayjs服务端",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts --port=7010 start --daemon --title=egg-server-bombayjs-server --ignore-stderr",
    "stop": "egg-scripts --port=7010 stop --title=egg-server-bombayjs-server",
    "dev": "egg-bin dev --port=7002",
    "debug": "egg-bin debug --port=7002",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --project . -c tslint.json",
    "clean": "ets clean"
  },
  "dependencies": {
    "@switchdog/egg-graphql": "^3.2.0-beta.4",
    "address": "^1.1.2",
    "bodybuilder": "^2.2.18",
    "detector": "^2.5.0",
    "dotenv": "^7.0.0",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.0",
    "egg-esearch": "^1.0.0",
    "egg-graphql": "^2.6.0",
    "egg-jwt": "^3.1.6",
    "egg-kafka-node": "^1.0.4",
    "egg-mongoose": "^3.2.0",
    "egg-multipart": "^2.9.0",
    "egg-redis": "^2.4.0",
    "egg-router-plus": "^1.3.1",
    "egg-scripts": "^2.6.0",
    "egg-validate": "^2.0.2",
    "egg-valparams": "^1.2.5",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "koa-is-json": "^1.0.0",
    "moment": "^2.24.0",
    "mongoose-schema-extend": "^0.2.2"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/supertest": "^2.0.0",
    "@types/bson": "^4.0.2",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-ci": "^1.8.0",
    "egg-bin": "^4.11.0",
    "egg-mock": "^3.16.0",
    "tslib": "^1.9.0",
    "tslint": "^5.0.0",
    "tslint-config-egg": "^1.0.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
  "author": "aoping",
  "license": "MIT"
}
